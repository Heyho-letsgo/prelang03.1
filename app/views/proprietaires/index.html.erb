<h1>Listing proprietaires</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proprietaires.each do |proprietaire| %>
      <tr>
        <td><%= proprietaire.name %></td>
        <td><%= link_to 'Show', proprietaire %></td>
        <td><%= link_to 'Edit', edit_proprietaire_path(proprietaire) %></td>
        <td><%= link_to 'Destroy', proprietaire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proprietaire', new_proprietaire_path %>
